// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// prisma/schema.prisma

model User {
  id        Int    @id @default(autoincrement())
  email     String @unique
  password  String
  firstName String? // rendre ce champ optionnel
  lastName  String? // rendre ce champ optionnel
}



